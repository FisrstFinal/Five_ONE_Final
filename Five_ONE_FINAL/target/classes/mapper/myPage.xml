<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="myPageDAO">

	<select id="history_list" resultType="videoplayDTO" parameterType="int">
		select * from video_play where video_code in (select video_code from history where member_code = #{code})
	</select>
	
	<select id="playlist_list" resultType="videoplayDTO" parameterType="int">
		select * from video_play where video_code in (select video_code from playlist where member_code = #{code})
	</select>
	
	<select id="good_list" resultType="videoplayDTO" parameterType="int">
		select * from video_play where video_code in (select video_code from good where member_code = #{code})
	</select>
	
	
	<select id="member_channel" resultType="user_channelDTO" parameterType="int">
		select * from user_channel where member_code = #{code}
	</select>
	
	<select id="channel_video" resultType="videoplayDTO" parameterType="int">
		select * from video_play where channel_code = #{code}
	</select>
	
	<select id="channel_sub" resultType="videoplayDTO" parameterType="int">
		select * from subscribe where channel_code = #{code}
	</select>
	
	<select id="reply_list" resultType="replyDTO" parameterType="int">
		select * from reply where member_code = #{code}
	</select>
	
</mapper>

