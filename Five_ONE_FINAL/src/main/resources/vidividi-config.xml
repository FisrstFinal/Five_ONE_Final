<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE configuration 
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"https://mybatis.org/dtd/mybatis-3-config.dtd">

<!-- typeAlias를 통해 type을 alias로 간단하게 사용할 수 있다. --> 
<configuration>
	<typeAliases>
		<typeAlias type="com.vidividi.model.VidividiDAO" alias="VidividiDAO"/>
		<typeAlias type="com.vidividi.variable.TestDTO" alias="TestDTO" />		
		
		<typeAlias type="com.vidividi.model.WatchDAO" alias="watchDAO" />
		<typeAlias type="com.vidividi.variable.ReplyDTO" alias="replyDTO" />
		
		<!-- myPage 관련 -->
		<typeAlias type="com.vidividi.variable.ReplyDTO" alias="replyDTO" />
		<typeAlias type="com.vidividi.variable.HistoryDTO" alias="historyDTO" />
		<typeAlias type="com.vidividi.variable.PlaylistDTO" alias="playlistDTO" />
		
		<!-- Member -->
		<typeAlias type="com.vidividi.model.MemberDAO" alias="memberdao" />
		<typeAlias type="com.vidividi.variable.MemberDTO" alias="memberdto" />
		<typeAlias type="com.vidividi.variable.LoginDTO" alias="logindto" />
		<typeAlias type="com.vidividi.variable.LoginHistoryDTO" alias="loginhistorydto" />
		
		<!-- Channel -->
		<typeAlias type="com.vidividi.model.ChannelDAO" alias="channeldao"/>
		<typeAlias type="com.vidividi.variable.ChannelDTO" alias="channeldto"/>
		
		<!-- VideoPlay -->
		<typeAlias type="com.vidividi.model.VideoPlayDAO" alias="videoplayDAO"/>
		<typeAlias type="com.vidividi.variable.VideoPlayDTO" alias="videoplayDTO" />
		
		<!-- subscribe -->
		<typeAlias type="com.vidividi.variable.SubscribeDTO" alias="subscribeDTO" />
		
		<!-- good -->
		<typeAlias type="com.vidividi.variable.GoodDTO" alias="GoodDTO" />
	</typeAliases>
</configuration>